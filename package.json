{
	"name": "form-generator",
	"version": "2.0.0",
	"description": "",
	"main": "./src/main.js",
	"scripts": {
		"lint": "eslint --fix src/**/*.ts",
		"test": "jest",
		"build": "npm run lint && rimraf dist && webpack && cpx src/appsscript.json dist && cpx \"src/**/*.html\" dist",
		"push": "npm run build && clasp push"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/syou-kou/FormGenerator.git"
	},
	"keywords": [
		"gas",
		"google-apps-script",
		"clasp"
	],
	"author": "fatman",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/syou-kou/FormGenerator/issues"
	},
	"homepage": "https://github.com/syou-kou/FormGenerator#readme",
	"devDependencies": {
		"@types/google-apps-script": "1.0.16",
		"@types/jest": "26.0.14",
		"@typescript-eslint/eslint-plugin": "^4.10.0",
		"@typescript-eslint/parser": "^4.10.0",
		"cpx": "1.5.0",
		"eslint": "7.11.0",
		"eslint-config-prettier": "6.13.0",
		"eslint-plugin-prettier": "3.1.4",
		"eslint-plugin-react": "^7.21.5",
		"gas-webpack-plugin": "1.2.0",
		"jest": "26.5.3",
		"prettier": "2.1.2",
		"rimraf": "3.0.2",
		"ts-jest": "26.4.1",
		"ts-loader": "8.0.5",
		"typescript": "4.0.3",
		"webpack": "4.42.0",
		"webpack-cli": "4.0.0"
	},
	"jest": {
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testRegex": "(src/.+(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	}
}